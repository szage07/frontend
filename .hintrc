{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-webkit-tap-highlight-color"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not edge <= 118",
    "not ios_saf <= 17.1",
    "not safari <= 17.1",
    "not and_ff <= 118"
  ]
}